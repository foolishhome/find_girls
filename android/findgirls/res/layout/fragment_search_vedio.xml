<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.yy.medical.activity.live.SearchItemActivity
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/searchItem">
        </com.yy.medical.activity.live.SearchItemActivity>

    <com.yy.a.widget.floatinglistview.FloatingGroupExpandableListView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/lv_vedio_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:childDivider="@color/line_grey"
        android:groupIndicator="@null"
        android:dividerHeight="1px"
        android:divider="@color/line_grey"
        android:overScrollMode="always"
        android:background="@color/white"
        />

</LinearLayout>